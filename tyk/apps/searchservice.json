{
    "name": "Search Service",
    "api_id": "search-service",
    "org_id": "default",
    "definition": {
      "location": "header",
      "key": "x-api-version"
    },
    "version_data": {
      "not_versioned": true,
      "versions": {
        "default": {
          "name": "default",
          "use_extended_paths": true
        }
      }
    },
    "proxy": {
      "listen_path": "/search/",
      "target_url": "http://searchservice:8000/",
      "strip_listen_path": true
    },
    "active": true,
    "use_keyless": true
}
  